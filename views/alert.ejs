<div class="container my-3">
  <!-- success msg -->
  <% if(flashMsg.success){ %>
  <div class="alert alert-warning alert-dismissible fade show" role="alert">
    <h6><%= flashMsg.success %></h6>
    <button
      type="button"
      class="btn-close"
      data-bs-dismiss="alert"
      aria-label="Close"
    ></button>
  </div>
  <% } %>
  <!-- fail msg -->
  <% if(flashMsg.fail){ %>
  <div class="alert alert-warning alert-dismissible fade show" role="alert">
    <h6><%= flashMsg.fail %></h6>
    <button
      type="button"
      class="btn-close"
      data-bs-dismiss="alert"
      aria-label="Close"
    ></button>
  </div>
  <% } %>
</div>
